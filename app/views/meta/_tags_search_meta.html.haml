- tags_name = local_assigns[:tags_name].present? ? local_assigns[:tags_name] : ['No tags']
- meta_title = APP_CONFIG['default_pre_title'] + " | " + "Tags: #{tags_name.join(', ')}"
- meta_keywords = APP_CONFIG['default_pre_keys'] + ", " + join_by(sort_only_by(keywords, :count), :name, 8)
- meta_desc = "All hentai form the private collection tagged by #{tags_name.join(', ')}. #{APP_CONFIG['default_pre_desc']}"

- meta title: meta_title, keywords: meta_keywords, description: meta_desc
