- tags_name = local_assigns[:tags_name].present? ? local_assigns[:tags_name] : ['No tags']
- title = APP_CONFIG['default_pre_title'] + " | " + "Tags: #{tags_name.join(', ')}"
- keyword = APP_CONFIG['default_pre_keys'] + ", " + join_by(sort_only_by(keywords, :count), :name, 8)
- desc = "All hentai form the private collection tagged by #{tags_name.join(', ')}"

- meta title: title, keywords: keywords, description: desc
