- image_name = local_assigns[:image_name]

- meta title: APP_CONFIG['default_pre_title'] + " | " + "Image: #{image_name}",
       keywords: APP_CONFIG['default_pre_keys'] + ", " + join_by(sort_only_by(keywords, :count), :name, 8),
       description: "Tiny part of all hentai form our collection named by #{image_name}"